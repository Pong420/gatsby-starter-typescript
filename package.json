    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "eslint-config-react-app": "^5.0.1",
    "gatsby": "^2.15.9",
    "gatsby-image": "^2.2.17",
    "gatsby-plugin-manifest": "^2.2.14",
    "gatsby-plugin-offline": "^3.0.3",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.13",
    "gatsby-plugin-sharp": "^2.2.20",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-source-filesystem": "^2.1.21",
    "gatsby-transformer-sharp": "^2.2.13",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "sass": "^1.22.10",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3"
  }
