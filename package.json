{
  "name": "gatsby-starter-typescript",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "precommit": "lint-staged",
    "component": "node scripts/component.js",
    "page": "node scripts/page.js",
    "get": "node scripts/type.js",
    "plugin": "node scripts/plugin.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rule 'no-console: [\"error\", { allow: [\"warn\", \"error\"] }]'  --max-warnings=0",
      "prettier --ignore-path .eslintignore --write"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "eslint-config-react-app": "^5.2.1",
    "gatsby": "^2.24.47",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-typescript": "^2.4.18",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "sass": "^1.26.10",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  }
}
